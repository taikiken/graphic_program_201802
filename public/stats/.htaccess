Options FollowSymLinks Includes
RewriteEngine on
RewriteBase /


# ub
# ==============================
RewriteRule ^ub_kansai/$ /stats/ub_kansai/2017a/ [R,L]
RewriteRule ^ub_kansai/standing/$ /stats/ub_kansai/2017a/standing/ [R,L]

RewriteRule ^ub_kansaibig6/$ /stats/ub_kansaibig6/2017a/ [R,L]
RewriteRule ^ub_kansaibig6/standing/$ /stats/ub_kansaibig6/2017a/standing/ [R,L]

RewriteRule ^ub_tohto/$ /stats/ub_tohto/2017a/ [R,L]
